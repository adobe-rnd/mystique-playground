{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "image",
  "type": "object",
  "properties": {
    "kind": {
      "type": "string",
      "const": "image"
    },
    "imageUrl": {
      "type": "string",
      "description": "The URL source of the image."
    },
    "src": {
      "type": "string",
      "description": "The URL source of the image."
    },
    "alt": {
      "type": "string",
      "description": "The alt text for the image."
